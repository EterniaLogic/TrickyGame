        android:layout_toRightOf="@+id/Leftbttn"
        android:layout_toEndOf="@+id/Leftbttn" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Swap"
        android:id="@+id/rightbttn"
        android:layout_alignTop="@+id/leftbttn"
        android:layout_toRightOf="@+id/leftbttn"
        android:layout_toEndOf="@+id/leftbttn" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Swap"
        android:id="@+id/Rightbttn"
        android:layout_alignTop="@+id/rightbttn"
        android:layout_toRightOf="@+id/rightbttn"
        android:layout_toEndOf="@+id/rightbttn" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:src="@mipmap/redhotel1"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView3"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:layout_alignLeft="@+id/imageView3"
        android:layout_alignStart="@+id/imageView3"
        android:layout_alignBottom="@+id/Rightbttn"
        android:layout_marginBottom="33dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="What is the password the the Red Hotel?"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/imageView5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6230"
        android:id="@+id/FLbttn"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:layout_marginTop="42dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0326"
        android:id="@+id/Lbttn"
        android:layout_alignTop="@+id/FLbttn"
        android:layout_toEndOf="@+id/FLbttn" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8730"
        android:id="@+id/Rbttn"
        android:layout_alignBottom="@+id/Lbttn"
        android:layout_toEndOf="@+id/Lbttn" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1876"
        android:id="@+id/FRbttn"
        android:layout_alignTop="@+id/Rbttn"
        android:layout_toEndOf="@+id/Rbttn" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/livesTxt"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
